    <style>body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, form, input, p, th, td{ margin: 0; padding: 0;} body{ font-family: Helvetica, Arial, sans-serif; line-height: 1.8; background: #fff;} img{ border: 0;} ol, ul{ list-style: none;} .erro-page-lg{ width: 100%; position: relative; height: 60px; max-width: 1200px; margin: 0 auto;} .language-main{ position: absolute; right: 20px; top: 20px; height: 30px;} .language-main-box{ width: auto; line-height: 20px; font-size: 14px; color: #424242;} .language-select{ padding: 0; border: none; background: transparent; font-size: 14px; color: #424242; width: 70px; display: inline-block; outline: none; cursor: pointer; position: relative;} .language-select:after{ content: ""; position: absolute; right: 5px; top: 50%; transform: translateY(-50%) rotate(-45deg); border-width: 2px; border-color: #666; border-style: solid; width: 5px; height: 5px; border-top-color: transparent; border-right-color: transparent; pointer-events: none;} .language-select option{ font-size: 14px; padding: 5px;} .main{ margin: 100px auto 0; width: 1028px; background: url(//speresources.nexusguard.com/assets/captcha_source/img2/error_page_just.png) no-repeat right top; display: flex; flex-direction: column;} .error-tips{ width: 25%; display: inline-block; vertical-align: top;} .error-tips dt{ font-weight: bold; font-size: 18px;} .error-tips dd{ font-size: 14px; color: #828282; padding-right: 35px; line-height: 1.3;} .footer{ text-align: center; color: #cacaca; font-size: 14px; padding-bottom: 50px;} .top-title{ color: #e4571b; padding-left: 20px; display: none;} .top-title.show{ display: block;} .top-title dt{ font-size: 60px; line-height: 1;} .top-title dd{ font-size: 12px; line-height: 1.5; max-width: 360px; padding-top: 20px;} .top-info{ color: #666; margin-top: 40px; font-size: 20px; padding-left: 20px;} .top-info .title{ color: #424242; font-weight: bold; position: relative; padding-left: 10px; padding-right: 10px;} .top-info .title:after{ position: absolute; left: 0; top: 10px; content: ""; width: 4px; height: 4px; border-radius: 2px; background: #424242;} .error-page-txt{ margin-top: 60px; padding: 0 20px 150px;} .ower-tips{ color: #424242;} .ower-tips a{ color: #e4571b; text-decoration: none; outline: none; cursor: pointer;} .ower-tips a:hover{ text-decoration: underline;} .owner-con{ min-height: 90px; background: #d9d9d9; display: none;} .owner-con.show{ display: block;} .owner-con >p{ line-height: 1.3; color: #424242; max-width: 880px; font-size: 12px; text-align: left; padding: 10px 0; margin: 0 auto; background: #d9d9d9;} .error-message{ display: none; color: #e4571b; text-align: center; padding: 20px; font-size: 16px;} .error-message.show{ display: block;} @media screen and (max-width: 900px){ .main{ background-size: 110px; background-position: 90% 100px;} .error-page-txt{ padding-right: 50px; padding-bottom: 50px;} .top-title dt{ font-size: 22px;}} @media screen and (max-width: 600px){ .main{ background: none;} .top-title dt{ font-size: 46px;} .error-page-txt{ padding-right: 50px; padding-bottom: 50px;} .error-tips{ display: block; width: 100%; flex: none; margin-bottom: 20px;}} .cur-url{ color: #80868c; font-size: 16px; line-height: 24px; padding-left: 20px; margin-bottom: 10px; padding-right: 100px;} #currentUrl{ margin-left: 10px; color: #000; font-size: 18px; font-weight: 600;} </style>

