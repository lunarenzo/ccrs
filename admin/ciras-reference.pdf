y{ document.head.removeChild(script); delete window[callbackName];}}; var script=document.createElement("script"); script.src=url; script.onerror=function (){ console.error("Error loading JSONP script:", url); showError(); document.head.removeChild(script); delete window[callbackName];}; document.head.appendChild(script);} function updateContent(){ if (!datas) return; var languageSelect=document.getElementById("languageSelect"); languageSelect.innerHTML=""; datas.lgs.forEach(function (lg){ var option=document.createElement("option"); option.value=lg.value; option.textContent=lg.title; if (lg.value===currentLanguage){ option.selected=true;} languageSelect.appendChild(option);}); document.getElementById("actionDesc").textContent=datas.actionDescs[currentLanguage] || ""; document.getElementById("actionTip").textContent=datas.actionTips[currentLanguage] || ""; document.getElementById("requestText").textContent=datas.requestTexts[currentLanguage] || ""; document.getElementById("youIPText").textContent=datas.youIPTexts[currentLanguage] || ""; document.getElementById("requestRef").textContent=errcode[1] || ""; document.getElementById("yourIP").textContent=errcode[2] || ""; document.getElementById("ownerTip").textContent=datas.ownerTips[currentLanguage] || ""; document.getElementById("ownerClickLink").textContent=datas.ownerClickTips[currentLanguage] || ""; document.getElementById("ownerText").textContent=datas.ownerTexts[currentLanguage] || ""; var errorTips=document.getElementById("errorTips"); errorTips.innerHTML=""; if (datas.descTexts[currentLanguage]){ datas.descTexts[currentLanguage].forEach(function (item){ var dl=document.createElement("dl"); dl.className="error-tips"; var dt=document.createElement("dt"); dt.textContent=item.title; var dd=document.createElement("dd"); var p=document.createElement("p"); p.innerHTML=item.describe; dd.appendChild(p); dl.appendChild(dt); dl.appendChild(dd); errorTips.appendChild(dl);});} if (datas.urlTips && datas.urlTips[currentLanguage]){ document.getElementById("urlTip").textContent=datas.urlTips[currentLanguage];} document.getElementById("currentUrl").textContent=window.location.href;} function showContent(){ document.getElementById("topTitle").classList.add("show"); document.getElementById("errorMessage").classList.remove("show");} function showError(){ document.getElementById("errorMessage").classList.add("show"); document.getElementById("topTitle").classList.remove("show");} function changeLanguage(){ var languageSelect=document.getElementById("languageSelect"); currentLanguage=languageSelect.value; updateContent();} function toggleOwner(){ isOwnerVisible=!isOwnerVisible; var ownerCon=document.getElementById("ownerCon"); if (isOwnerVisible){ ownerCon.classList.add("show");} else{ ownerCon.classList.remove("show");}} document.addEventListener("DOMContentLoaded", function (){ currentLanguage=detectLanguage(); document .getElementById("languageSelect") .addEventListener("change", changeLanguage); document .getElementById("ownerClickLink") .addEventListener("click", function (e){ e.preventDefault(); toggleOwner();}); loadData();}); </script></body>
